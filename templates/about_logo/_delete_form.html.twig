<form method="post" action="{{ path('about_logo_delete', {'id': about_logo.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <a href="{{ path('about_logo_edit', {'id': about_logo.id}) }}" class="btn btn-info">Изменить</a>
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ about_logo.id) }}">
    <button class="btn btn-danger">Удалить</button>
</form>
