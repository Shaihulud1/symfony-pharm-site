{% extends 'base.admin.html.twig' %}

{% block title %}Landing{% endblock %}

{% block body %}
    <h1>Landing</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ landing.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ landing.name }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ landing.code }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ landing.phone }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ landing.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ landing.createdAt ? landing.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ landing.updatedAt ? landing.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('landing_index') }}">back to list</a>

    <a href="{{ path('landing_edit', {'id': landing.id}) }}">edit</a>

    {{ include('landing/_delete_form.html.twig') }}
{% endblock %}
